#include <bits/stdc++.h>
using namespace std;

int main() {
	int x,y,z,w;
	cout << "enter value of x: ";
	cin >> x;
	cout << "enter value of y: ";
	cin >> y;
	cout << "enter value of z: ";
	cin >> z;
	cout << "enter value of w: ";
	cin >> w;
	 if(x==w || y==w || z==w || (x+y)==w ||(x+z)==w || (y+z)==w || (x+y+z)==w)
	 {
	     cout << "YES" << endl;
	 }
	 else
	 {
	     cout << "NO" << endl;
	 }
	
	return 0;

}
