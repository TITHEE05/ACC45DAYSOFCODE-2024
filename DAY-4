#include <bits/stdc++.h>
using namespace std;

int main() {
	int a,b,c;
	cout << "give the number of the three topics the chef has prepared: ";
	cin >> a >> b >> c;
	int x;
	cout << "give topic number on which the chef has to speak: ";
	cin >>x;

	if(x==a || x==b || x==c)
	{
	    cout << "yes" << endl;
	}
	else
	{
	    cout << "no" << endl;
	}
	
	return 0;
}
