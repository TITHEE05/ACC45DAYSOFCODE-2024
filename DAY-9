#include <bits/stdc++.h>
using namespace std;

int main() {
	int x,y;
	cout << "rent of cooler for a mounth is x = ";
	cin >> x;
	cout<< "price pf purchasing cooler is y = ";
	cin >> y;
	
	if(x<y)
	{
	    cout << "maximum number of mounths the cooler can be purchased is: " << y/x << endl;
	}
	else 
	{
	    cout << "cant purchase cooler" << endl;
	}
	return 0;
}
