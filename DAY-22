#include <iostream>
using namespace std;

int main(){
int n;
cout << "number of terms in polynomial: ";
cin >> n;
int a[5];
for(int i=0;i<n;i++)
{
    cout << "value of coefficient of polynomial: ";
    cin >> a[i];
}
for(int i=1; i<=n; i++)
{
    if(a[n-i]!=0)
    {
         cout << "the heighest power of x with non zero coefficient is: " << n-i << endl;
         break;
    }
}
return 0;
}
