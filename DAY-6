#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int x,y; //time required ti solve problem a and b
	cout << "time taken to solve quesiton A: ";
	cin >> x;
	cout << "time taken to solve question B: ";
	cin >> y;
	int pa,pb,total_points;
	//points scored for problem a
	pa = 500 - (x*2);
	//points scored for problem b
	pb = 1000 - (y*4);
	total_points = pa + pb;
	cout << "TOTAL POINTS scored : " << total_points << endl;
	
	return 0;
}
